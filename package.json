{
  "name": "apisauce",
  "version": "0.2.0",
  "description": "A JSON API client for JavaScript. No, no. Not like that. Like this.",
  "repository": {
    "type": "git",
    "url": "https://github.com/skellock/apisauce.git"
  },
  "main": "./dist/apisauce.js",
  "scripts": {
    "test": "ava",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc ava",
    "dist": "npm run clean && npm run build && npm run docs",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json",
    "clean": "rm -rf dist docs",
    "build": "BABEL_ENV=production gulp build"
  },
  "keywords": [
    "axios",
    "api",
    "network",
    "http"
  ],
  "author": "Steve Kellock <steve@kellock.ca>",
  "license": "MIT",
  "files": [
    "dist/apisauce.js",
    "lib/apisauce.d.ts"
  ],
  "typings": "./lib/apisauce.d.ts",
  "dependencies": {
    "axios": "^0.11.1",
    "ramda": "^0.21.0",
    "ramdasauce": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "docdash": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rollup": "^1.8.0",
    "jsdoc": "^3.4.0",
    "nyc": "^6.4.0",
    "rollup": "^0.26.3",
    "xyz": "^0.5.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  }
}
